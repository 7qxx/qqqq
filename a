local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
-- \\ ========

-- // Variables
local MovementSpeed = 800
local Character = LocalPlayer.Character
local Humanoid = Character.Humanoid
local HRP = Character.HumanoidRootPart
local OldCf = HRP.CFrame
local Noclip = false
local PlatformStand = false
local Players = game:GetService("Players")
local ServerName = game:GetService("ReplicatedStorage").ServerName.Value
local AutoKick = true
-- \\ ========

local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")


local TweenService = game:GetService("TweenService")
local Area = ""
local Settings = {
    Movement = {
        TweenSpeed = 700,
        UnderOffset = Vector3.new(0, -700, 0) 
    },
    General = {
        Noclip = true,
        PlatformStand = true
    }
}

local TweenT = false

function GetBodyPosition(HumanoidRootPart)
    local BodyPosition = Instance.new("BodyPosition")
    BodyPosition.Position = HumanoidRootPart.Position
    BodyPosition.Parent = HumanoidRootPart
    return BodyPosition
end

RunService.Stepped:Connect(function()
    if Settings.General.Noclip then
        for I, V in pairs(LocalPlayer.Character:GetChildren()) do
            if V:IsA("BasePart") then
                V.CanCollide = false
            end
        end
    end
end)

RunService.Stepped:connect(function()
    if AutoKick then
       for i,plr in pairs(game.Players:GetPlayers()) do
        pcall(function()
        magnitude = (Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).magnitude
        if magnitude < 30 and plr.Name ~= Character.Name then
        LocalPlayer:Kick("someone came within your distance.. " .. plr.Name)
          TeleportService:Teleport(6808589067, LocalPlayer)
          LogD("Proximity Kick, user triggered: " .. plr.Name)
        end
        end)
       end
      end
    end)

    local function TweenReal(CF)
        LogD("Tweening")
        TweenT = true
        pcall(function()
            local Character = LocalPlayer.Character
            local Humanoid = Character.Humanoid
            local HRP = Character.HumanoidRootPart
            local UnderPosition = CF.Position + Settings.Movement.UnderOffset
    
            local BodyPosition = GetBodyPosition(HRP)
    
            local UnderTweenInfo = TweenInfo.new((HRP.Position - UnderPosition).Magnitude / Settings.Movement.TweenSpeed, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
            local ToCFTweenInfo = TweenInfo.new((UnderPosition - CF.Position).Magnitude / Settings.Movement.TweenSpeed, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
    
            BodyPosition.MaxForce = Vector3.new(9e9, 9e9, 9e9)
            BodyPosition.P = 1000000
    
            local OldNoclip = Settings.General.Noclip
            local OldPlatformStand = Settings.General.PlatformStand
            Settings.General.Noclip = true
            Settings.General.PlatformStand = true

            local UnderTween = TweenService:Create(BodyPosition, UnderTweenInfo, {Position = UnderPosition})
            UnderTween:Play()
            UnderTween.Completed:Wait()
    
            local ToCFTween = TweenService:Create(BodyPosition, ToCFTweenInfo, {Position = CF.Position})
            ToCFTween:Play()
            ToCFTween.Completed:Wait()
            local StartTime = os.time()
            repeat
                wait()
            until
                (HRP.Position - CF.Position).Magnitude <= 1 or os.time() - StartTime >= 20 or Humanoid.Health < 1
            BodyPosition.MaxForce = Vector3.new(0, 0, 0)
            Settings.General.Noclip = OldNoclip
            Settings.General.PlatformStand = OldPlatformStand
            BodyPosition:Destroy()
        end)
        TweenT = false
    end
local function Tween(CF)
local HumanoidRootPart = LocalPlayer.Character.Character.HumanoidRootPart
  TweenReal(HumanoidRootPart.CFrame.X,-700,HumanoidRootPart.CFrame.Z)
TweenReal(CF.X,-700,CF.Z)

TweenReal(CF.X,CF.Y,CF.Z)

end
